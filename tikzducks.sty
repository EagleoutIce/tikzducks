%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%
% Accessories
%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

%1: antennae colour
\newcommand*{\addalien}[1]{%
	\draw[line width=2,color=#1] (1.00,1.83) -- (1.20,2.30);%
	\draw[line width=2,color=#1] (0.65,1.92) -- (0.60,2.35);%
	\path[fill=#1] (1.20,2.30) circle (0.07);%
	\path[fill=#1] (0.60,2.35) circle (0.07);%
}

%1: fabric colour
\newcommand*{\addhat}[1]{%
	\path[fill=#1] (0.90,1.95) ellipse (0.46 and 0.0893);%
	\path[fill=#1] (0.90,2.20) ellipse (0.25 and 0.0714);%
	\path[fill=#1] (1.15,2.20) rectangle (0.65,1.95);%
}

%1: hair colour, fure will be 75% lighter
\newcommand*{\addunicorn}[1]{%
	\path[draw=#1,fill=#1!35!white,line width=1,rotate=25] 
	(1.50,1.4375)--(1.60,1.8375)--(1.70,1.4375);%
}

%1: frame colour
%2: glass colour
\newcommand*{\addsunglasses}[2]{%
	\path[draw=#1,line width=1] (0.93,1.62) -- (1.30,1.50);%
	\draw[line width=1,color=#1] (0.73,1.67) arc (65:92:0.20);%
	\path[draw=#1,fill=#2,line width=1,rotate=-20] (0.23,1.7675) circle (0.125);%
	\path[draw=#1,fill=#2,line width=1,rotate=-20] (-0.06,1.74) circle (0.1143);%
}

%1: frame colour
\newcommand*{\addglasses}[1]{%
	\path[draw=#1,line width=1] (0.93,1.62) -- (1.30,1.50);%
	\draw[line width=1,color=#1] (0.73,1.67) arc (65:92:0.20);%
	\path[draw=#1,line width=1,rotate=-20] (0.23,1.7675) circle (0.125);%
	\path[draw=#1,line width=1,rotate=-20] (-0.06,1.74) circle (0.1143);%
}

%1: fabric colour
\newcommand*{\addtie}[1]{%
	\draw[line width=3,color=#1] (0.60,1.00) -- (0.50,0.60);%
}

%1: Ice cream 1
%2: Ice cream 2
%3: Ice cream 3
\newcommand*{\addicecream}[3]{%
	\path[draw=Sienna,fill=Goldenrod,line width=0.5,rotate=-20] (0.35,1.20)--(0.50,0.60)--(0.75,1.20);%
	\path[draw=Sienna, fill=Goldenrod, rotate=-20,line width=0.5] (0.55,1.20) ellipse (0.20 and 0.09);%
	\path[fill=#1] (0.82,1.03) circle (0.12);%
	\path[fill=#2] (1.02,0.95) circle (0.12);%
	\path[fill=#3] (0.95,1.06) circle (0.12);%
}

%1: book colour
%2: title text
\newcommand*{\addbook}[2]{%
		\path[fill=#1,rotate=-20] (0.40,1.20) rectangle (0.80,0.60);%
		\node[rotate=-20, color=white] at (0.88,0.70) {#2};%
}

%1: background colour hat
%2: colour of decoration stars
\newcommand*{\addmagichat}[2]{%
	\path[preaction={fill, #1},rotate=-5,pattern=fivepointed stars, pattern color=#2] (0.36,1.95)--(0.86,2.95)--(1.20,1.85);%
}

\newcommand*{\addmagicwand}{%
	\draw[line width=2.5,color=black] (0.90,0.90) -- (0.60,0.40);%
	\draw[line width=2.5,color=white] (0.85,0.8167) -- (0.80,0.7333);%
}

%1: water colour
\newcommand*{\addwater}[1]{%
	\path[draw=#1, line width=5] (1.00,0.40) ellipse (0.80 and 0.28);%
	\path[draw=#1, line width=3] (1.00,0.40) ellipse (1.00 and 0.45);%
	\path[draw=#1, line width=2] (1.00,0.40) ellipse (1.20 and 0.60);%
	\path[draw=#1, line width=1] (1.00,0.40) ellipse (1.40 and 0.75);%
}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%
% some wrapper for likly combinations of attributes
%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

%1: hair colour
\newcommand*{\makeeinstein}[1]{%
	\addcrazyhair{#1}%
	\addeyebrow{#1}%
}

\newcommand*{\makeknuth}{%
	\duck{Wheat}%
	\addtshirt{white!93!black}%
	\addjacket{DarkRed}%
	\addrecedinghair{gray!50!white}%
	\addglasses{brown!20!LightGray}%
	\addbook{brown!70!black}{\scalebox{0.5}{\TeX}}%
}

%1: background colour hat
%2: colour of decoration stars
\newcommand*{\addwizzard}[2]{%
	\addmagichat{#1}{#2}%
	\addmagicwand%
}

